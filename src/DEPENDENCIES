#
# global definitions
#
include $(top_srcdir)/../../DEPENDENCIES

#
# private definitions
#

#
# libkern target
#
$(LIBKERNTARGET) $(LIBKERNSTMP):
	cd $(top_srcdir)/../../sys/libkern; \
		$(MAKE) CPU="$(CPU)" libkern$(CPU).a

#
# libgeneric target
#
$(LIBGENERICTARGET) $(LIBGENERICSTMP):
	cd $(top_srcdir)/drivers/generic; \
		$(MAKE) libgeneric.a
